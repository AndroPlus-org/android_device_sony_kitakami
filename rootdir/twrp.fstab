# mount point	fstype	device			[device2]   fstype2

/boot emmc /dev/block/bootdevice/by-name/boot flags=backup=1;display="kernel"
/recovery emmc /dev/block/bootdevice/by-name/FOTAKernel
/system ext4 /dev/block/bootdevice/by-name/system  flags=usermrf
/system_image emmc /dev/block/bootdevice/by-name/system
/data ext4 /dev/block/bootdevice/by-name/userdata length=-16384
/cache ext4 /dev/block/bootdevice/by-name/cache flags=display="Cache";backup=0;usermrf
/boot/modem_fs1 emmc /dev/block/bootdevice/by-name/modemst1
/boot/modem_fs2 emmc /dev/block/bootdevice/by-name/modemst2
/boot/ssd emmc /dev/block/bootdevice/by-name/ssd
/firmware vfat /dev/block/bootdevice/by-name/modem
/external_sd vfat /dev/block/mmcblk1p1 /dev/block/mmcblk1 flags=display="Micro SDcard";storage;wipeingui;removable;andsec
